   "source": [
    "f=open(\"births.csv\",\"r\")\n",
    "text=f.read()\n",
    "strings=text.split(\"\\n\")\n",
    "strings\n",
    "\n",
    "weekday_birth ={}\n",
    "no_header = strings[1:len(strings)]\n",
    "for each in no_header:\n",
    "    list = each.split(\",\")\n",
    "    day_of_week = list[3]\n",
    "    births = int(list[4])\n",
    "    \n",
    "    if day_of_week in weekday_birth:\n",
    "        weekday_birth[day_of_week]=weekday_birth[day_of_week]+births\n",
    "    else:\n",
    "        weekday_birth[day_of_week]=births\n",
    "    \n",
    "list\n",
    "\n",
    "print(weekday_birth)\n",
    "\n"
   ]
